// env("DATABASE_URL")
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://admin:admin@localhost:3306/weatherDB"
}

model History {
  id          Int      @id @default(autoincrement())
  city        String
  temperature Float
  date        DateTime
}
